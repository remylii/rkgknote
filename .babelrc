{
  "sourceMaps": "inline",
  "plugins": [
    "transform-flow-strip-types"
  ],
  "presets": ["es2015", "react"],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "production": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-remove-console"
      ]
    }
  }
}
